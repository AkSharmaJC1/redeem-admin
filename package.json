{
	"name": "redeemed-event-admin",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite --port 6003",
		"build": "tsc -b && vite build --mode development",
		"build:development": "tsc -b && vite build --mode development",
		"build:production": "tsc -b && vite build --mode production",
		"build:staging": "tsc -b && vite build --mode staging",
		"start": "vite preview --port 6003",
		"lint": "eslint . --fix --max-warnings=0",
		"format": "prettier . --write",
		"prepare": "husky install",
		"check-types": "tsc --pretty --noEmit",
		"lint:check": "eslint src/**/*.{js,jsx,ts,tsx,json}",
		"lint:fix": "eslint --fix src/**/*.{js,jsx,ts,tsx,json}",
		"test-all": "npm run check-prettier  && npm run check-lint && npm run check-types",
		"format:fix": "prettier --write src/**/*.{js,jsx,ts,tsx,css,scss,json} --config ./.prettierrc",
		"format:check": "prettier --check src/**/*.{js,jsx,ts,tsx,css,scss,json} --config ./.prettierrc",
		"type-check": "tsc --noEmit",
		"check:all": "npm run lint:check && npm run format:check && npm run type-check",
		"fix:all": "npm run lint:fix && npm run format:fix"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": [
			"eslint --fix",
			"prettier --write"
		],
		"*.{json,css,scss,md}": [
			"prettier --write"
		]
	},
	"dependencies": {
		"@hookform/resolvers": "^4.1.3",
		"@react-google-maps/api": "^2.20.6",
		"@reduxjs/toolkit": "^2.6.1",
		"@sentry/react": "^9.6.1",
		"axios": "^1.8.4",
		"bootstrap": "^5.3.3",
		"browser-image-compression": "^2.0.2",
		"crypto-js": "^4.2.0",
		"date-fns": "^4.1.0",
		"express": "^5.1.0",
		"http": "^0.0.1-security",
		"i18next": "^24.2.3",
		"i18next-http-backend": "^3.0.2",
		"libphonenumber-js": "^1.12.6",
		"lodash": "^4.17.21",
		"lottie-react": "^2.4.1",
		"moment": "^2.30.1",
		"path": "^0.12.7",
		"react": "^18.3.1",
		"react-chartjs-2": "^5.3.0",
		"react-date-picker": "^11.0.0",
		"react-date-range": "^2.0.1",
		"react-dom": "^18.3.1",
		"react-hook-form": "^7.54.2",
		"react-hot-toast": "^2.5.2",
		"react-i18next": "^15.4.1",
		"react-infinite-scroll-component": "^6.1.0",
		"react-otp-input": "^3.1.1",
		"react-phone-input-2": "^2.15.1",
		"react-redux": "^9.2.0",
		"react-router-dom": "^7.4.0",
		"react-time-picker": "^7.0.0",
		"redux-persist": "^6.0.0",
		"redux-persist-transform-encrypt": "^5.1.1",
		"sass": "^1.86.0",
		"secure-ls": "^2.0.0",
		"swiper": "^11.2.6",
		"url": "^0.11.4",
		"yup": "^1.6.1"
	},
	"devDependencies": {
		"@eslint/js": "^9.21.0",
		"@types/crypto-js": "^4.2.2",
		"@types/lodash": "^4.17.16",
		"@types/react": "^19.0.10",
		"@types/react-date-range": "^1.4.10",
		"@types/react-dom": "^19.0.4",
		"@vitejs/plugin-react": "^4.3.4",
		"eslint": "^9.21.0",
		"eslint-plugin-react-hooks": "^5.1.0",
		"eslint-plugin-react-refresh": "^0.4.19",
		"globals": "^15.15.0",
		"husky": "^9.1.7",
		"lint-staged": "^15.5.0",
		"prettier": "^3.5.3",
		"typescript": "~5.7.2",
		"typescript-eslint": "^8.24.1",
		"vite": "^6.2.0"
	}
}
